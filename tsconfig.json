{
  "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "outDir": "./dist",
      "baseUrl": "./src",
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
  },
  "include": [
      "./src/**/*"
  ],
  "exclude": [
      "node_modules",
       "dist",
       "**/*.test.ts"
  ]
}
